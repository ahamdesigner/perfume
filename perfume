perfume={}
perfume.customize()
perfume.upgrade()

perfume.opinion = opinion
perfume.ഡിസൈനർ = designer
perfume.ദക്ഷിണ = samarpanam

perfume.customize=(){
 perfume.choice()
}
perfume.choice=(perfume.opinion){
 if(perfume.opinion){
  perfume.trial
 }
 if(perfume.trial){
  perfume.custom
 }
 if(perfume.custom){
  perfume.design
 }
}

perfume.upgrade=(){
 perfume.limitedEdition(perfume.ദക്ഷിണ)
}
perfume.limitedEdition=(സമർപ്പണം){
 perfume.ഡിസൈനർ
}
